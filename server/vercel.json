{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://sahilatahar-book-store.vercel.app"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}